{
    "mcpServers": {
        "context7": {
            "command": "npx",
            "args": ["-y", "@upstash/context7-mcp"]
        },
        "sequential-thinking": {
            "command": "npx",
            "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
        },
        "mcp-compass": {
            "command": "npx",
            "args": ["-y", "@liuyoshio/mcp-compass"]
        },
        "docker-mcp": {
            "command": "uvx",
            "args": ["docker-mcp"]
        },
        "browsermcp": {
            "command": "npx",
            "args": ["@browsermcp/mcp@latest"]
        }
    }
}
